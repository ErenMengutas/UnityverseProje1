<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="E2E suiti">
   <test name="E2E olarak işaretlenmiş testleri çalıştırma">
       <groups>
           <run>
               <include name="E2E"></include>
           </run>
       </groups>
       <packages>
           <package name="tests.*"></package>
       </packages>

   </test>
</suite>